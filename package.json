{
    "name": "lightspeed",
    "version": "0.5.0",
    "main": "index.js",
    "license": "MIT",
    "peerDeependencies": {
        "react": ">=16.14.0",
        "react-dom": ">=16.14.0",
        "react-chatbotify": ">=2.0.0-beta.37"
    },
    "scripts": {
        "build": "yarn clean && NODE_ENV=production yarn ts-node node_modules/.bin/webpack",
        "build-dev": "yarn clean && yarn ts-node node_modules/.bin/webpack",
        "package": "yarn build && tar -C dist -cvf dist/extension-$npm_package_name-$npm_package_version.tar resources",
        "package-dev": "yarn build-dev && tar -C dist -cvf dist/extension-$npm_package_name-$npm_package_version.tar resources",
        "clean": "rm -rf dist"
    },
    "devDependencies": {
        "@types/react": "^16.8.5",
        "argo-ui": "git+https://github.com/argoproj/argo-ui.git",
        "css-loader": "^6.7.3",
        "mini-css-extract-plugin": "^2.7.2",
        "node-sass": "^8.0.0",
        "raw-loader": "^4.0.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-moment": "^0.9.7",
        "sass": "^1.34.1",
        "sass-loader": "^13.2.0",
        "style-loader": "^3.3.1",
        "ts-loader": "9.4.2",
        "ts-node": "^10.8.1"
    },
    "dependencies": {
        "@rcb-plugins/markdown-renderer": "^0.2.0",
        "marked": "^15.0.12",
        "react-chatbotify": "^2.0.0-beta.37",
        "typescript": "^4.9.3",
        "uuid": "^11.1.0",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.0",
        "yaml": "^2.7.1"
    }
}
