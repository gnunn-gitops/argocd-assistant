{
    "ts-node": {
        "compilerOptions": {
          "module": "commonjs"
        }
    },
    "compilerOptions": {
        "baseUrl": ".",
        "module": "esnext",
        "moduleResolution": "node",
        "target": "es2020",
        "jsx": "react",
        "allowJs": true,
        "strict": false,
        "noUnusedLocals": true,
        "sourceMap": true
      },
    "include": ["src/*"],
    "exclude": ["node_modules"]
  }
